@model IEnumerable<A_ZAcademy.Models.InstructorRequest>

@{
    ViewData["Title"] = "ConsumeInstL";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}

<h1>ConsumeInstL</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
