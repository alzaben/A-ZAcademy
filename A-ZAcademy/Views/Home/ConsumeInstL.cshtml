                @Html.DisplayFor(modelItem => item.Resume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InstructorRequestId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InstructorRequestId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InstructorRequestId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
